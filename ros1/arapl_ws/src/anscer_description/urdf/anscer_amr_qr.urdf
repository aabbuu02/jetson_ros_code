<link name ="ultrasonic_sensorfl">
</link>
<joint name="ultrasonic_sensorfl_link_to_base_link" type="fixed">
  <parent link="base_link"/>

  <child link="ultrasonic_sensorfl"/>

  <origin xyz=".352 .130 -0.0395" rpy ="0 0 0"/>
</joint>

<gazebo reference="ultrasonic_sensorfl">
   <sensor type="ray" name="ultrasonic_sensorfl">
      <pose>0 0 0 0 0 0</pose>
      <visualize>true</visualize>
      <update_rate>50</update_rate>
      <ray>
         <scan>
            <horizontal>
               <samples>10</samples>
               <resolution>1</resolution>
               <min_angle>-0.03</min_angle>
               <max_angle>0.03</max_angle>
            </horizontal>
            <vertical>
               <samples>10</samples>
               <resolution>1</resolution>
               <min_angle>-0.03</min_angle>
               <max_angle>0.03</max_angle>
            </vertical>
         </scan>
         <range>
            <min>0.01</min>
            <max>2</max>
            <resolution>0.02</resolution>
         </range>
      </ray>
      <plugin filename="libgazebo_ros_range.so" name="gazebo_ros_range">
         <gaussianNoise>0.005</gaussianNoise>
         <alwaysOn>true</alwaysOn>
         <updateRate>50</updateRate>
         <topicName>sensor/sonar_scanfl</topicName>
         <frameName>ultrasonic_sensorfl</frameName>
         <radiation>INFRARED</radiation>
         <fov>0.1</fov>
      </plugin>
   </sensor>
   </gazebo>

