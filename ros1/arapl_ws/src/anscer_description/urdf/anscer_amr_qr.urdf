    <joint name="lidar_front_to_base_base_link" type="fixed">
      <parent link="base_link"/>
      <child link="lidar_link_front"/>
  <origin rpy="0 0 -0.8" xyz="0.9 -0.455 0"/>
    </joint>
<link name="lidar_link_front">
    </link>



<joint name="lidar_rear_to_base_base_link" type="fixed">
      <parent link="base_link"/>
      <child link="lidar_link_rear"/>
  <origin rpy="0 0 2.4" xyz="-0.9 0.455 0 "/>
    </joint>
<link name="lidar_link_rear">
    </link>


 <gazebo reference="lidar_link">
        <sensor type="ray" name="Velodyne-HDL32E">
          <pose>0 0 0 0 0 0</pose>
          <visualize>false</visualize>
          <update_rate>15</update_rate>
          <ray>
            <scan>
              <horizontal>
                <samples>200</samples>
                <resolution>1</resolution>
                <min_angle>-1.57</min_angle>
                <max_angle>1.57</max_angle>
              </horizontal>
              <vertical>
                <samples>32</samples>
                <resolution>1</resolution>
                <min_angle>0.0</min_angle>
                <max_angle>0.26</max_angle>
              </vertical>
            </scan>
            <range>
              <min>0.9</min>
              <max>50</max>
              <resolution>1</resolution>
            </range>
            <noise>
              <type>gaussian</type>
              <mean>0.0</mean>
              <stddev>0.0</stddev>
            </noise>
          </ray>
          <plugin name="gazebo_ros_laser_controller" filename="libgazebo_ros_velodyne_laser.so">
            <topicName>scan_cloud</topicName>
            <frameName>lidar_link</frameName>
            <organize_cloud>${organize_cloud}</organize_cloud>
            <min_range>0.9</min_range>
            <max_range>50</max_range>
            <gaussianNoise>0</gaussianNoise>
          </plugin>
        </sensor>

    </gazebo>






<!--  -->

<link name="imu_link">
</link>


<joint name="imu_to_base_base_link" type="fixed">
<parent link="base_link"/>
<child link="imu_link"/>
 <origin rpy="0 0 0" xyz="0 0 0"/>
</joint>

                    <gazebo reference="imu_link">
                      <gravity>true</gravity>
                      <sensor name="imu_sensor" type="imu">
                        <always_on>true</always_on>
                        <update_rate>50</update_rate>
                        <visualize>true</visualize>
                        <topic>__default_topic__</topic>
                        <plugin filename="libgazebo_ros_imu_sensor.so" name="imu_plugin">
                          <topicName>imu</topicName>
                          <bodyName>imu_link</bodyName>
                          <updateRateHZ>60</updateRateHZ>
                          <gaussianNoise>0.0</gaussianNoise>
                          <xyzOffset>0 0 0</xyzOffset>
                          <rpyOffset>0 0 0</rpyOffset>
                          <frameName>imu_link</frameName>
                        </plugin>
                        <pose>0 0 0 0 0 0</pose>
                      </sensor>
                    </gazebo>


                    <!-- differential drive -->

                    <gazebo>
         <plugin name="differential_drive_controller" filename="libgazebo_ros_diff_drive.so">
           <commandTopic>cmd_vel</commandTopic>
           <rosDebugLevel>Debug</rosDebugLevel>
           <odometryTopic>odom</odometryTopic>
           <odometryFrame>odom</odometryFrame>
           <robotBaseFrame>base_footprint</robotBaseFrame>
           <publishWheelTF>false</publishWheelTF>
           <publishWheelJointState>true</publishWheelJointState>
           <legacyMode>false</legacyMode>
           <updateRate>60</updateRate>
           <alwaysOn>true</alwaysOn>
           <leftJoint>left_mWheel_to_base_link</leftJoint>
           <rightJoint>right_mWheel_to_base_link</rightJoint>
           <wheelSeparation>0.320</wheelSeparation>
           <wheelDiameter>0.120</wheelDiameter>
           <wheelAcceleration>10</wheelAcceleration>
           <wheelTorque>1000</wheelTorque>
         </plugin>
        </gazebo>

  <joint name="camera_front_optical_joint" type="fixed">
    <!-- these values have to be these values otherwise the gazebo camera image
        won't be aligned properly with the frame it is supposedly originating from -->
    <origin xyz="0 0 0" rpy="-1.57 0 -1.57"/>
    <parent link="camera_front_link"/>
    <child link="camera_front_link_optical"/>
  </joint>

  <link name="camera_front_link_optical">
  </link>
  
<joint name="camera_front_optical_to_base_link" type="fixed">
  <parent link="base_link"/>
  <child link="camera_front_link"/>
  <origin xyz="0.650 0 0.3" rpy="0 1.57 0"/>
</joint>

<link name="camera_front_link">

</link>

<gazebo reference="camera_front_link">
  <static>true</static>
    <material>Gazebo/white</material>
  <turnGravityOff>false</turnGravityOff>


  <sensor type="depth" name="camera">
    <always_on>true</always_on>
    <update_rate>30.0</update_rate>

    <camera>
        <horizontal_fov>1.309</horizontal_fov>
      <image>
        <format>R8G8B8</format>
        <width>640</width>
        <height>480</height>
      </image>
     
      <noise>base_
        <type>gaussian</type>
        <mean>0.0</mean>
        <stddev>0.005</stddev>
      </noise>
    </camera>

   <plugin name="camera_plugin" filename="libgazebo_ros_camera.so">
          <alwaysOn>true</alwaysOn>
        
          <updateRate>0.0</updateRate>
          <cameraName>camera_front</cameraName>
          <imageTopicName>/camera/color/front_image_raw</imageTopicName>
          <cameraInfoTopicName>/camera/color/front_camera_info</cameraInfoTopicName>
         
          <frameName>camera_front_link_optical</frameName>
            <hackBaseline>0.0</hackBaseline>
            <distortionK1>0.0</distortionK1>
            <distortionK2>0.0</distortionK2>
            <distortionK3>0.0</distortionK3>
            <distortionT1>0.0</distortionT1>
            <distortionT2>0.0</distortionT2>
            <Cx>0.0</Cx>
            <Cy>0.0</Cy>
            <focalLength>0.0</focalLength>
      </plugin>
  </sensor>
</gazebo>

